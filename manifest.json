{
  "manifest_version": 2,

  "name": "I'd Tab That",
  "description": "An extension to save and share Chrome sessions",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
